<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_cardrow_inquiry_name"
        android:gravity="center"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_cardrow_inquiry_bedroomCount"
        android:gravity="center"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_cardrow_inquiry_floorArea"
        android:gravity="center"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_cardrow_inquiry_lotArea"
        android:gravity="center"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_cardrow_inquiry_status"
        android:gravity="center"/>

    <TextView
        android:text="SET TRIP SCHEDULE"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_cardrow_inquiry_setTripSched"
        android:visibility="gone"
        android:gravity="center"/>


</LinearLayout>

</android.support.v7.widget.CardView>
